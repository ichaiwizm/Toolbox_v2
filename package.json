{
  "name": "toolbox",
  "version": "0.1.0",
  "description": "Toolbox fullstack application with React frontend and Node.js backend",
  "scripts": {
    "dev": "npm-run-all --parallel backend frontend",
    "frontend": "cd frontend && npm run frontend",
    "backend": "cd backend-node && npm start",
    "backend-debug": "cd backend-node && TOOLBOX_LOG_LEVEL=DEBUG npm start",
    "install-all": "npm install && cd frontend && npm install && cd ../backend-node && npm install",
    "test": "cd backend-node && npm test"
  },
  "devDependencies": {
    "npm-run-all": "^4.1.5"
  },
  "keywords": ["toolbox", "react", "nodejs", "file-management"],
  "author": "",
  "license": "MIT"
}
